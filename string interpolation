using System;
class virtusa {
  static void Main(string[] args) {
    string a = Console.ReadLine();
    string b = Console.ReadLine();
    string c = $"Complete word is : {a} {b}";
    Console.WriteLine(c);
  }
}
/* 
    input - sun
            light
            
    output - Complete word is : sun light  
    
*/
