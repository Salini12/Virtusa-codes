using System;
using System.Linq;
class virtusa{
    static void Main(string [] args){
        int[] a = new int[4];
        for(int i=0;i<4;i++){
            a[i] =  Convert.ToInt32(Console.ReadLine());
        }
        Array.Sort(a);
        foreach(int i in a){
            Console.WriteLine(i);
        }
    }
}
/* 
 input - 4 3 2 1
 output-1 2 3 4   
                 */
